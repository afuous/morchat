{
  "name": "morchat-web",
  "version": "0.0.1",
  "description": "The web client for MorChat",
  "scripts": {
    "build": "./node_modules/.bin/gulp build",
    "build-prod": "NODE_ENV=production ./node_modules/.bin/gulp build",
    "gulp": "./node_modules/.bin/gulp",
    "gulp-prod": "NODE_ENV=production ./node_modules/.bin/gulp",
    "one-by-one": "for page in $(echo $(cd src; echo */) | sed -e 's/\\///g' | tr ' ' \"\\n\" | grep -v '\\(shared\\|util\\)'); do npm run gulp-prod build-$page; done",
    "watch": "./node_modules/.bin/gulp watch",
    "vendor": "./node_modules/.bin/gulp vendor",
    "vendor-prod": "NODE_ENV=production ./node_modules/.bin/gulp vendor",
    "postinstall": "npm run vendor; [ -e src/config.json ] || echo '{\"imgurClientId\":\"\"}' > src/config.json",
    "trailing": "find src -name '*.js' -o -name '*.json' | xargs -n1 sed -i -e 's/\\s\\+$//'"
  },
  "dependencies": {
    "autolinker": "^3.1.0",
    "axios": "^0.19.0",
    "babel-runtime": "^6.9.2",
    "dompurify": "^2.0.7",
    "prop-types": "^15.7.2",
    "radium": "^0.18.1",
    "react": "^15.3.0",
    "react-bootstrap": "^0.30.0",
    "react-dom": "^15.1.0",
    "react-modal": "^1.4.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-sounds": "^1.1.1",
    "redux-thunk": "^2.1.0",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-root-import": "^4.0.1",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "event-stream": "^3.3.4",
    "gulp": "^4.0.0",
    "gulp-streamify": "^1.0.2",
    "gulp-tap": "^0.1.3",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1"
  }
}
